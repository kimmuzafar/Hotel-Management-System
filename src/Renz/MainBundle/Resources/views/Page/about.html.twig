{% extends '::base.html.twig' %}

{% block content %}

<!-- Parallax Effect -->
<script type="text/javascript">$(document).ready(function(){$('#parallax-pagetitle').parallax("50%", -0.55);});</script>

<section class="parallax-effect">
  <div id="parallax-pagetitle" style="background-image: url({{ asset('bundles/main/images/parallax/parallax-01.jpg') }});">
    <div class="color-overlay"> 
      <!-- Page title -->
      <div class="container">
        <div class="row">
          <div class="col-sm-12">
            <ol class="breadcrumb">
              <li><a href="{{ path('renz_main_index_index') }}">{{ 'site.topnav.home' | trans }}</a></li>
              <li class="active">{% if app.request.locale=='ar' %}{{ entity.titlear }}{% else %}{{ entity.title }}{% endif %}</li>
            </ol>
            <h1>{% if app.request.locale=='ar' %}{{ entity.titlear }}{% else %}{{ entity.title }}{% endif %}</h1>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>


<div class="container">
  <div class="row"> 
    
    <!-- Blog -->
    <section class="blog mt50">
      <div class="col-md-9"> 
        <!-- Article -->
        <article>
          <div class="row">
            
            <div class="col-md-12">
              
              <p>{% if app.request.locale=='ar' %}{{ entity.longDetailar | raw }}{% else %}{{ entity.longDetail | raw }}{% endif %}</p>
              
            </div>
            
          </div>
        </article>
        
      </div>
    </section>
    
    
    <!-- Aside -->
    <aside class="mt50">
      <div class="col-md-3"> 
		
        {{ render(controller('MainBundle:Common:facilitiesatrenzhotel')) }}
        
      </div>
    </aside>
  </div>
</div>

{% endblock %}